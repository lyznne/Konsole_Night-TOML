{
  "name": "konsole-toml",
  "displayName": "Konsole TOML",
  "description": "syntax hightlighting for TOML from Konsole Night Color Theme",
  "version": "1.0.0",
  "publisher": "Enoslyznne",
  "icon": "images/icon.png",
  "keywords": [
    "toml",
    "TOML",
    "TOML Syntax Highlighting",
    "TOML Syntax Highlighting for Konsole Night Color Theme",
    "Konsole Night Color Theme",
    "hightlighting",
    "new toml",
    "syntaxhighlting"
  ],
    "author": {
    "name": "Enos Lyznne",
    "url": "https://www.github.com/lyznne",
    "urls": [
      "https://enos.vercel.app"
    ],
    "twitter": "https://www.twitter.com/enoslyznne"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lyznne/Konsole_Night-TOML.git"
  },
  "engines": {
    "vscode": "^1.92.0"
  },
     "categories": [
        "Programming Languages"
    ],
  "contributes": {
     "languages": [
            {
                "id": "toml",
                "aliases": [
                    "toml",
                    "TOML"
